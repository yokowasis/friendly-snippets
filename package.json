{
  "name": "friendly-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "plaintext",
          "markdown",
          "tex",
          "html",
          "global",
          "all"
        ],
        "path": "./snippets/global.json"
      },
      {
        "language": "javascript,javascriptreact",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript,typescriptreact",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "html",
        "path": "./snippets/html.json"
      },
      {
        "language": "html,php",
        "path": "./snippets/php.json"
      },
      {
        "language": "javascriptreact,typescriptreact,javascript,typescript",
        "path": "./snippets/jsfamily.json"
      },
      {
        "language": "javascriptreact,typescriptreact",
        "path": "./snippets/react.json"
      },
      {
        "language": [
          "license"
        ],
        "path": "./snippets/license.json"
      }
    ]
  }
}
