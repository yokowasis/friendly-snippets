{
  "typetemplate": {
    "prefix": "tyt",
    "body": [
      "/**",
      " * @template T",
      " * @typedef ${1:TypeName}",
      " * @type {{a : string}}",
      " */"
    ]
  },
  "typeimport": {
    "prefix": "typeimport",
    "body": ["/**", " * @typedef { import(\"${1:./Types.js}\")}", " */"]
  },
  "typedef": {
    "prefix": "typedef",
    "body": ["/** @typedef {${1:someTypeOrImport}} ${2:TypeName} */"]
  },
  "typeWrap": {
    "prefix": "ty",
    "body": ["/** @type {${1:TypeName}} */ ($TM_SELECTED_TEXT)"]
  },
  "type": {
    "prefix": ":",
    "body": ["/** @type {${1:TypeName}} */ "]
  }
}
